{
    "rules": {
      "disallowTabs": false,
      "max-len": ["error", { "code": 100 }],
      "space-before-blocks": ["error", "never"],
      "linebreak-style": ["error", "windows"],
      "indent": ["error", "tab"],
      "quotes": ["error", "double", { "avoidEscape": true }]
    },
    "extends": "eslint-config-google"
}
